{
  "name": "@stokei/api",
  "version": "1.0.0",
  "description": "Stokei API",
  "author": "Douglas Pereira Bernardes",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker-compose:up": "docker-compose up -d",
    "docker-compose:dev-utils": "docker-compose up -d postgresql",
    "db:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/database/prisma/schema.prisma",
    "db:migration:run": "prisma migrate deploy --schema=./src/database/prisma/schema.prisma",
    "db:migration:dev:run": "prisma migrate dev --preview-feature --schema=./src/database/prisma/schema.prisma",
    "db:migration:reset": "prisma migrate reset --schema=./src/database/prisma/schema.prisma",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/subgraph": "^0.3.1",
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/cqrs": "^8.0.3",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/throttler": "^2.0.0",
    "@prisma/client": "^3.15.0",
    "@stokei/nestjs": "^1.0.22",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "helmet": "^5.0.2",
    "nanoid": "^3.1.30",
    "prisma": "^3.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1"
    }
  }
}
