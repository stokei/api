{
  "name": "@stokei/api",
  "version": "1.0.0",
  "description": "Stokei API",
  "author": "Douglas Pereira Bernardes",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --debug --watch",
    "start:prod": "node dist/main",
    "stripe:webhooks:start": "stripe listen --forward-to localhost:4000/v1/webhooks/stripe",
    "docker-compose:up": "docker-compose up -d",
    "docker-compose:dev-utils": "docker-compose up -d postgresql",
    "db:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/database/prisma/schema.prisma",
    "db:seeds:run": "prisma db seed",
    "db:migration:run": "prisma migrate deploy --schema=./src/database/prisma/schema.prisma",
    "db:migration:dev:run": "prisma migrate dev --preview-feature --schema=./src/database/prisma/schema.prisma",
    "db:migration:reset": "prisma migrate reset --schema=./src/database/prisma/schema.prisma",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register ./src/database/seeds/server.ts"
  },
  "dependencies": {
    "@apollo/gateway": "^2.3.0",
    "@apollo/server": "^4.3.2",
    "@apollo/subgraph": "^2.3.0",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^4.0.0",
    "@prisma/client": "^4.12.0",
    "@stokei/nestjs": "^1.3.8",
    "@tus/file-store": "^1.0.0-beta.1",
    "@tus/server": "^1.0.0-beta.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.1.2",
    "body-parser": "^1.20.0",
    "cache-manager": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "graphql": "^16.6.0",
    "helmet": "^5.0.2",
    "multer-s3": "^3.0.1",
    "nanoid": "^3.1.30",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "stripe": "^12.1.1",
    "ts-node-dev": "^2.0.0",
    "tus-js-client": "^3.0.1",
    "tus-node-server": "^0.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.0-next.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1"
    }
  }
}
