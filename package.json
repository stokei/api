{
  "name": "@stokei/api",
  "version": "1.0.0",
  "description": "Stokei API",
  "author": "Douglas Pereira Bernardes",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --debug --watch",
    "start:prod": "node dist/main",
    "stripe:webhooks:start": "stripe listen --forward-to localhost:4000/v1/webhooks/stripe",
    "docker-compose:up": "docker-compose up -d",
    "docker-compose:dev-utils": "docker-compose up -d postgresql",
    "db:studio": "prisma studio --schema=./src/database/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/database/prisma/schema.prisma",
    "db:seeds:run": "prisma db seed",
    "db:migration:run": "prisma migrate deploy --schema=./src/database/prisma/schema.prisma",
    "db:migration:dev:run": "prisma migrate dev --preview-feature --schema=./src/database/prisma/schema.prisma",
    "db:migration:reset": "prisma migrate reset --schema=./src/database/prisma/schema.prisma",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "check:updates": "ncu",
    "run:upgrades": "ncu -u",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register ./src/database/seeds/server.ts"
  },
  "dependencies": {
    "@apollo/gateway": "^2.4.8",
    "@apollo/server": "^4.7.4",
    "@apollo/subgraph": "^2.4.8",
    "@aws-sdk/abort-controller": "^3.374.0",
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.374.0",
    "@nestjs/apollo": "^12.0.3",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.0.3",
    "@nestjs/core": "^10.0.3",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/graphql": "^12.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^7.0.4",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.1.0",
    "@prisma/client": "^5.3.1",
    "@sendgrid/mail": "^7.7.0",
    "@stokei/nestjs": "^1.4.0",
    "@tus/file-store": "^1.0.0-beta.1",
    "@tus/server": "^1.0.0-beta.6",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "dataloader": "^2.2.2",
    "dayjs": "^1.11.8",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "graphql": "^16.7.1",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.0.0",
    "multer-s3": "^3.0.1",
    "nanoid": "3.3.6",
    "npm-check-updates": "^16.10.12",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "stripe": "^12.10.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tus-js-client": "^3.1.0",
    "tus-node-server": "^0.9.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.74",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^5.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1"
    }
  }
}
